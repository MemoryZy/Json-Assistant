<idea-plugin>
    <actions>
        <action id="MemoryZy.JsonAssistant.actions.JavaBeanToJson"
                icon="JsonAssistantIcons.JSON"
                class="cn.memoryzy.json.actions.JavaBeanToJsonAction">
            <keyboard-shortcut keymap="$default" first-keystroke="alt N"/>
            <keyboard-shortcut keymap="$default" first-keystroke="alt shift N"/>
        </action>

        <action id="MemoryZy.JsonAssistant.actions.JsonToJavaBean"
                class="cn.memoryzy.json.actions.JsonToJavaBeanAction">
        </action>

        <group id="MemoryZy.groups.JsonAssistant.JavaBeanToJson">
            <separator/>
            <reference ref="MemoryZy.JsonAssistant.actions.JavaBeanToJson"/>
            <separator/>

            <add-to-group group-id="EditorPopupMenu" anchor="first"/>
        </group>

        <group id="MemoryZy.groups.JsonAssistant.JsonToJavaBean">
            <separator/>
            <reference ref="MemoryZy.JsonAssistant.actions.JsonToJavaBean"/>
            <separator/>

            <add-to-group group-id="NewGroup" relative-to-action="NewFile" anchor="before"/>
        </group>
    </actions>
</idea-plugin>
